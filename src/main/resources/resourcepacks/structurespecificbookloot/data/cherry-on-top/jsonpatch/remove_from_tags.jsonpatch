@version "1";
@target {
    "path": {
        "start": "tags/enchantment/",
        "end": ".json"
    }
};

foreach (configEntry in cherryontop_config.enchantedBookLootImprovements.enchantmentStructureMap) {
    $values = arrays.removeIf($values, (value) -> strings.split(configEntry, ",")[0] == value);
}